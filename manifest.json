{
    "manifest_version": 3,
    "name": "Tab Order Customizer",
    "version": "1.0",
    "description": "Customize tab order.",
    "permissions": [ "tabs", "storage" ],
    "icons": {
        "16": "icons/icon16.png",
        "48": "icons/icon48.png",
        "128": "icons/icon128.png"
    },
    "background": {
        "service_worker": "background.js",
        "type": "module"
    },
    "options_page": "options.html"
}
